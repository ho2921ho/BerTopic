    "    if df['2021'][word] > min_cnt and df['2022'][word] > min_cnt:\n",
    "        conf_2021 = df['2021'][word]/df['2021'][-1]\n",
    "        conf_2022 = df['2022'][word]/df['2022'][-1]\n",
    "        ratio_2022 = conf_2022 / conf_2021\n",
    "        ratio_2021 = conf_2021 / conf_2022\n",
    "        key = word\n",
    "        x.append(ratio_2021)\n",
    "        y.append(ratio_2022)\n",
    "        keys.append(key)"
   ]
